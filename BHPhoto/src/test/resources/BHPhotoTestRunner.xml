<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="BH Photo Automation Script" parallel="tests">
    <parameter name="useCloudEnv" value="true"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="60.0"/>
    <parameter name="url" value=" https://www.bhphotovideo.com/"/>
    <test name = "Search Test">
        <classes>
            <class name="PageTest.AudioVisualTest"/>
            <class name="PageTest.CameraTest"/>
            <class name="PageTest.ComputerTest"/>
            <class name="PageTest.LightingTest"/>
            <class name="PageTest.MobileTest"/>
            <class name="PageTest.OpticsTest"/>
            <class name="PageTest.ProAudioTest"/>
            <class name="PageTest.ProVideoTest"/>
            <class name="PageTest.TVEntertainmentTest"/>
            <class name="PageTest.BottomTest"/>
            <class name="PageTest.FooterTest"/>
            <class name="PageTest.LowerTest"/>
        </classes>
    </test>
</suite>